{
  "name": "sudoku-chess",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"yarn client\" \"yarn server\" ",
    "build": "concurrently --kill-others \"yarn prod-client\" \"yarn server\" ",
    "client": "cd client && yarn start",
    "prod-client": "cd client && yarn install &&yarn build",
    "server": "cd server && yarn install && nodemon index.ts"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1"
  }
}
